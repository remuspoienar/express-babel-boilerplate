{
  "name": "babel-node-boilerplate",
  "version": "1.0.0",
  "description": "Node.js Babel boilerplate",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "nodemon --exec babel-node src/index.js",
    "serve": "npm run build && node dist/index.js",
    "test": "yarn build && yarn mocha --require @babel/register",
    "watch:build": "webpack --watch",
    "watch:server": "yarn run nodemon \"./dist/index.js\" --watch \"./dist\"",
    "dev-server": "yarn run concurrently \"yarn run watch:server \" \"yarn run watch:build\""
  },
  "author": "Remus Poienar <remus.poienar.not@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@webpack-cli/serve": "^1.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "mocha": "7.1.1",
    "nodemon": "^1.17.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0"
  },
  "type": "module"
}
